    "@types/node": "^18.16.19",
    "@types/three": "^0.149.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "bumpp": "^8.2.1",
    "critters": "^0.0.16",
    "eslint": "^8.44.0",
    "typescript": "~5.1.6",
